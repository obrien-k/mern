ations.map((recommendation, index) => (\n                            <tr key={index}>\n                                <td>\n                                    <a href={`communities.js?id=${recommendation.groupID}`}>{recommendation.groupName}</a>\n                                    {' '} (by {recommendation.username})\n                                    {renderTags(recommendation.tagList)}\n                                </td>\n                            </tr>\n                        ))}\n                    </table>\n                </div>\n            )}\n        </div>\n    );\n}\n\nexport default Recommendations;\n","/Users/kyleobrien/git/mern/client/src/components/pages/Polls.js",[],"/Users/kyleobrien/git/mern/client/src/components/pages/VanityAlbum.js",[],"/Users/kyleobrien/git/mern/client/src/components/pages/FeaturedAlbum.js",[],"/Users/kyleobrien/git/mern/client/src/components/pages/News.js",["267"],"// News.js\nimport React, { useState, useEffect } from 'react';\n\nfunction NewsItem({ newsItem, isAdmin }) {\n    const [isHidden, setIsHidden] = useState(false);\n\n    const toggleHidden = () => {\n        setIsHidden(!isHidden);\n    };\n\n    return (\n        <div id={`news${newsItem.newsID}`} className=\"box news_post\">\n            <div className=\"head\">\n                <strong>{newsItem.title}</strong>\n                {isAdmin && (\n                    <span> - <a href={`tools.php?action=editnews&id=${newsItem.newsID}`} className=\"brackets\">Edit</a></span>\n                )}\n                <span style={{ float: 'right' }}>\n                    <a href=\"#\" onClick={toggleHidden} className=\"brackets\">{isHidden ? 'Show' : 'Hide'}</a>\n                </span>\n            </div>\n            {!isHidden && (\n                <div id={`newsbody${newsItem.newsID}`} className=\"pad\">\n                    {newsItem.body}\n                </div>\n            )}\n        </div>\n    );\n}\n\nfunction News() {\n    const [news, setNews] = useState([]);\n    const newsCount = 5; // Set this to the desired maximum number of news items to be displayed\n    const isAdmin = true; // This should be determined based on user permissions.\n\n    useEffect(() => {\n        // Fetch news data here\n        // For now, I'm setting dummy data\n\n        setNews([\n            {\n                newsID: 1,\n                title: 'News Title',\n                body: 'News Body',\n                newsTime: '2023-06-26'\n            },\n            // ...more news items\n        ]);\n    }, []);\n\n    return (\n        <div>\n            {news.slice(0, newsCount).map((newsItem, index) => (\n                <NewsItem key={index} newsItem={newsItem} isAdmin={isAdmin} />\n            ))}\n        </div>\n    );\n}\n\nexport default News;\n","/Users/kyleobrien/git/mern/client/src/actions/auth.js",[],"/Users/kyleobrien/git/mern/client/src/actions/alert.js",[],"/Users/kyleobrien/git/mern/client/src/actions/types.js",[],"/Users/kyleobrien/git/mern/client/src/config/config.js",[],"/Users/kyleobrien/git/mern/client/src/components/sections/forum/ForumPage.js",["268"],"import React from 'react';\nimport { Link, useParams } from 'react-router-dom';\nimport { useForumDataById } from '../../../hooks/useForumDataById'; // Using the new hook here\nimport './ForumList.css';\nimport NewTopicForm from './NewTopicForm';\n\nconst ForumPage = () => {\n  const { forumId } = useParams();\n\n  const { data: forumData, isLoading, errorMessage } = useForumDataById(forumId); // Using the new hook here\n\n  if (isLoading) {\n    return <div>Loading...</div>;\n  }\n\n  if (errorMessage) {\n    return <div>Error: {errorMessage}</div>;\n  }\n\n  if (!forumData) {\n    return <div>Forum not found</div>;\n  }\n\n  return (\n    <div className=\"thin\">\n      <h2>\n        <Link to=\"/forums\">Forums</Link> &gt; {forumData.name}\n      </h2>\n      <div className=\"linkbox\">\n        <Link to={`/forums/${forumData.id}/new-topic`} className=\"brackets\